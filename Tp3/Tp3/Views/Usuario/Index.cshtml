@{
    ViewData["Title"] = "Inicio";
}

<div class="text-center">
    <h1 class="display-4">Cadeteria El Correcaminos</h1>
    <form asp-action="Create" asp-controller="Cadete" method="post">
        <legend>Alta Cadete</legend>
        <label>Desea dar de alta un cadete?</label>
        <input type="submit" value="Si" />
    </form>

    <form asp-action="Create" asp-controller="Pedido" method="post">
        <legend>Generar Pedido</legend>
        <label>Desea generar un pedido?</label>
        <input type="submit" value="Si" />
    </form>
</div>